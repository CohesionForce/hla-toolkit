    <modelIdentification>
        <name>JLVC Warfare</name>
        <type>FOM</type>
        <version>1.0r3</version>
        <modificationDate>2014-05-23</modificationDate>
        <securityClassification>unclassified</securityClassification>
        <purpose>Adds JLVC specific versions of Fire, Detonation, and Battle Damage interactions</purpose>
        <applicationDomain></applicationDomain>
        <description>Adds JLVC specific versions of Fire, Detonation, and Battle Damage interactions</description>
        <useLimitation></useLimitation>
        <useHistory>1.0r2 Release Notes - Update Purpose and Description in Identification Table.</useHistory>
        <useHistory>1.0r3 - Remove Lat/Long DDM dimensions from Fire/Det/Damage interactions.</useHistory>
        <other></other>
    </modelIdentification>
    <interactions>
        <interactionClass>
            <name>HLAinteractionRoot</name>
            <interactionClass>
                <name>MunitionDetonation</name>
                <interactionClass>
                    <name>JLVC_MunitionDetonation</name>
                    <sharing>Subscribe</sharing>
                    <transportation>reliable</transportation>
                    <order>Receive</order>
                    <semantics>MunitionDetonation interactions published within the JLVC with JLVC agreed upon attribute.</semantics>
                    <parameter>
                        <name>MissionID</name>
                        <dataType>NullTerminatedASCIIString</dataType>
                        <semantics>Optional. Associated mission identifier.</semantics>
                    </parameter>
                    <interactionClass>
                        <name>ExtendedMunitionDetonation</name>
                        <sharing>Subscribe</sharing>
                        <transportation>reliable</transportation>
                        <order>Receive</order>
                        <semantics>An extended interaction for MunitionDetonation.</semantics>
                        <parameter>
                            <name>HitAimPoint</name>
                            <dataType>HitAimPointEnum8</dataType>
                            <semantics>REQUIRED. Guided weapons use this field to indicate whether they hit their intended aimpoint or not</semantics>
                        </parameter>
                        <parameter>
                            <name>RelativeBulletLocation</name>
                            <dataType>RelativePositionStruct</dataType>
                            <semantics>This field contains the relative location of the point of closest approach of a bullet to the target's center of gravity. Cardinality 1+</semantics>
                        </parameter>
                        <parameter>
                            <name>WeaponFailCode</name>
                            <dataType>WeaponFailCode8</dataType>
                            <semantics>Indicates why a weapon failed.</semantics>
                        </parameter>
                        <parameter>
                            <name>IntendedTarget</name>
                            <dataType>NullTerminatedASCIIString</dataType>
                            <semantics>DMPI_ID; Cardinality 0-1</semantics>
                        </parameter>
                    </interactionClass>
                </interactionClass>
            </interactionClass>
            <interactionClass>
                <name>WeaponFire</name>
                <interactionClass>
                    <name>JLVC_WeaponFire</name>
                    <sharing>PublishSubscribe</sharing>
                    <transportation>reliable</transportation>
                    <order>Receive</order>
                    <semantics>WeaponFire interactions published within the JLVC with JLVC agreed upon attribute.</semantics>
                    <parameter>
                        <name>MissionID</name>
                        <dataType>NullTerminatedASCIIString</dataType>
                        <semantics>Optional. Associated mission identifier.</semantics>
                    </parameter>
                </interactionClass>
            </interactionClass>
            <interactionClass>
                <name>BattleDamageAssessment</name>
                <sharing>Subscribe</sharing>
                <transportation>reliable</transportation>
                <order>Receive</order>
                <semantics>Provides information on the damage resulting from an attack.</semantics>
                <parameter>
                    <name>DamageResultReason</name>
                    <dataType>DamageReasonEnum32</dataType>
                    <semantics>The reason for the damage or lack thereof</semantics>
                </parameter>
                <parameter>
                    <name>ComputedDamageResult</name>
                    <dataType>DISDamageStatusEnum32</dataType>
                    <semantics>The damage resulting from the attack.</semantics>
                </parameter>
                <parameter>
                    <name>MissionID</name>
                    <dataType>NullTerminatedASCIIString</dataType>
                    <semantics>Optional. Associated mission identifier.</semantics>
                </parameter>
                <parameter>
                    <name>DamageDetails</name>
                    <dataType>DamageRepairStruct</dataType>
                    <semantics>The details of the damage, how long it will take to repair and the level of maintenance organization which will perform the repair.</semantics>
                </parameter>
                <parameter>
                    <name>DamageMethod</name>
                    <dataType>AttritionDamageEnum16Array</dataType>
                    <semantics>Optional. Informs how the damage was generated.</semantics>
                </parameter>
                <parameter>
                    <name>NewDamageState</name>
                    <dataType>DISDamageStatusEnum32</dataType>
                    <semantics>Provides the new damage state.</semantics>
                </parameter>
                <parameter>
                    <name>DamageCount</name>
                    <dataType>UnsignedInteger16</dataType>
                    <semantics></semantics>
                </parameter>
                <parameter>
                    <name>EventIdentifier</name>
                    <dataType>EventIdentifierStruct</dataType>
                    <semantics>An ID, generated by the issuing federate, used to associate the BattleDamageAssessment interaction with related fire and detonation events.</semantics>
                </parameter>
                <parameter>
                    <name>PreviousDamageState</name>
                    <dataType>DISDamageStatusEnum32</dataType>
                    <semantics>The damage state of the object prior to the event.</semantics>
                </parameter>
                <parameter>
                    <name>ReportingObjectIdentifier</name>
                    <dataType>RTIobjectId</dataType>
                    <semantics>The RTI Object Identifier of the reporting object.</semantics>
                </parameter>
            </interactionClass>
            <interactionClass>
                <name>FacilityBattleDamageAssessment</name>
                <sharing>Subscribe</sharing>
                <transportation>reliable</transportation>
                <order>Receive</order>
                <semantics>specifies damage to a facility</semantics>
                <parameter>
                    <name>TargetIdentifier</name>
                    <dataType>HLAASCIIstring</dataType>
                    <semantics>Equivalent to the Target Identifier in the USMTF TNO specification. This field can represent data from a variety of planning systems including BE Number (e.g. /1234DF8765), BE WITH SUFFIX (e.g. /S1234F1234599), BE WITH BEN SUFFIX (e.g. /N1234-12345AA001), PLSS (e.g. X12345), PIN (e.g. /E12345001), ABCA (e.g. /AAA1234), Theater Target Number (e.g. /T1A12345AZ), HULTEC (e.g. /H1234ABCEF123), and FIBE (e.g. / 2134AA1689)</semantics>
                </parameter>
                <parameter>
                    <name>DamageResultReason</name>
                    <dataType>DamageReasonEnum32</dataType>
                    <semantics>Reason for NO damage to the facility</semantics>
                </parameter>
                <parameter>
                    <name>EventIdentifier</name>
                    <dataType>EventIdentifierStruct</dataType>
                    <semantics>Event identifier of the munition detonation.</semantics>
                </parameter>
                <parameter>
                    <name>NewStructuralStatus</name>
                    <dataType>Integer32</dataType>
                    <semantics>Post Event structural Status %. 100% is undamaged and 0% is totally destroyed.</semantics>
                </parameter>
                <parameter>
                    <name>PreviousStructuralStatus</name>
                    <dataType>Integer32</dataType>
                    <semantics>Pre-Event structural Status %. 100% is undamaged and 0% is totally destroyed.</semantics>
                </parameter>
                <parameter>
                    <name>MissionID</name>
                    <dataType>HLAASCIIstring</dataType>
                    <semantics>Optional. Associated mission identifier.</semantics>
                </parameter>
                <parameter>
                    <name>DamageMethod</name>
                    <dataType>AttritionDamageEnum16</dataType>
                    <semantics>Optional. Informs how the damage was generated.</semantics>
                </parameter>
            </interactionClass>
            <interactionClass>
                <name>Attrition</name>
                <sharing>Neither</sharing>
                <transportation>reliable</transportation>
                <order>Receive</order>
                <semantics>Entity attrition specified by a source other than that which owns the entity, i.e. MESL events to kill sim entities</semantics>
                <parameter>
                    <name>attritionStruct</name>
                    <dataType>AttritionStruct</dataType>
                    <semantics>the collection of damaged entity id\'s, kill types, and damage reasons</semantics>
                </parameter>
                <parameter>
                    <name>from</name>
                    <dataType>EntityIdentifierStruct</dataType>
                    <semantics>Who sent this interaction</semantics>
                </parameter>
                <parameter>
                    <name>numberOfRecords</name>
                    <dataType>UnsignedInteger16</dataType>
                    <semantics>number of attritionStructs to expect after this</semantics>
                </parameter>
                <parameter>
                    <name>sequenceNumber</name>
                    <dataType>UnsignedInteger16</dataType>
                    <semantics>Combined with the \"from\" gives a unique identification of this interactio</semantics>
                </parameter>
            </interactionClass>
            <interactionClass>
                <name>BattleDamageRepair</name>
                <sharing>PublishSubscribe</sharing>
                <transportation>reliable</transportation>
                <order>Receive</order>
                <semantics>Performs handshaking for repair requests and replies</semantics>
                <parameter>
                    <name>damagedEntity</name>
                    <dataType>RTIobjectId</dataType>
                    <semantics>RTI name of entity requesting repairs.</semantics>
                </parameter>
                <parameter>
                    <name>repairCount</name>
                    <dataType>UnsignedInteger16</dataType>
                    <semantics>Number of items in the repairDetails Array.</semantics>
                </parameter>
                <parameter>
                    <name>repairDetails</name>
                    <dataType>DamageRepairStruct</dataType>
                    <semantics>Array that reflects current state of damage/repair.</semantics>
                </parameter>
                <parameter>
                    <name>repairEntity</name>
                    <dataType>RTIobjectId</dataType>
                    <semantics>RTI name of entity that is/will help repair damaged system.</semantics>
                </parameter>
                <parameter>
                    <name>repairHandling</name>
                    <dataType>DamageHandlingEnum8</dataType>
                    <semantics>used to indicated MedEvac or similar special handling OPTIONAL.</semantics>
                </parameter>
                <parameter>
                    <name>repairNotes</name>
                    <dataType>NullTerminatedASCIIString</dataType>
                    <semantics>Notes regarding damage, repair. One per Code. OPTIONAL.</semantics>
                </parameter>
                <parameter>
                    <name>statusCode</name>
                    <dataType>DamageRepairTransactionEnum8</dataType>
                    <semantics>Enumeration value for repair status.  If StatusCode has a value of DOWReport then field RepairTime in DamageRepairStruct in Parameter RepairDetails will be used to indicate the time until death (in minutes).</semantics>
                </parameter>
            </interactionClass>
            <interactionClass>
                <name>LogisticsAssessment</name>
                <sharing>Neither</sharing>
                <transportation>reliable</transportation>
                <order>Receive</order>
                <semantics>Interaction to pass supply information</semantics>
                <parameter>
                    <name>EntityOwner</name>
                    <dataType>RTIobjectId</dataType>
                    <semantics>the owner making a logisticsassessment interaction</semantics>
                </parameter>
                <parameter>
                    <name>Resources</name>
                    <dataType>LogisticsStruct</dataType>
                    <semantics>Structure defining the type of supply and supply level.</semantics>
                </parameter>
                <parameter>
                    <name>numSupplies</name>
                    <dataType>UnsignedInteger16</dataType>
                    <semantics>the number of supplies.</semantics>
                </parameter>
                <interactionClass>
                    <name>PersonnelAndEquipment</name>
                    <sharing>PublishSubscribe</sharing>
                    <transportation>reliable</transportation>
                    <order>Receive</order>
                    <semantics>Used to report the assets of a \"unit\"</semantics>
                    <parameter>
                        <name>Equipment</name>
                        <dataType>EquipmentResourceStructArray</dataType>
                        <semantics>Equipment owned by the unit</semantics>
                    </parameter>
                    <parameter>
                        <name>NumEquipmentEntries</name>
                        <dataType>UnsignedInteger32</dataType>
                        <semantics>The number of records in the equipment list</semantics>
                    </parameter>
                    <parameter>
                        <name>NumPersonnelEntries</name>
                        <dataType>UnsignedInteger32</dataType>
                        <semantics>Number of records in the personnel list</semantics>
                    </parameter>
                    <parameter>
                        <name>Personnel</name>
                        <dataType>PersonnelResourceStructArray</dataType>
                        <semantics>Personnel owned by the unit</semantics>
                    </parameter>
                </interactionClass>
            </interactionClass>
            <interactionClass>
                <name>LogisticsResupply</name>
                <sharing>PublishSubscribe</sharing>
                <transportation>reliable</transportation>
                <order>Receive</order>
                <semantics>Interaction to resupply an entity</semantics>
                <parameter>
                    <name>Resupplyee</name>
                    <dataType>RTIobjectId</dataType>
                    <semantics>The object being resupplied</semantics>
                </parameter>
                <parameter>
                    <name>numSupplies</name>
                    <dataType>UnsignedInteger16</dataType>
                    <semantics>The number of supplies provided</semantics>
                </parameter>
                <parameter>
                    <name>Resources</name>
                    <dataType>LogSupplyStructArray</dataType>
                    <semantics>The details of the supplies provided</semantics>
                </parameter>
            </interactionClass>
        </interactionClass>
    </interactions>
